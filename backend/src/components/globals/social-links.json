{
    "collectionName": "components_globals_social_links",
    "info": {
        "displayName": "Social Links",
        "icon": "link"
    },
    "options": {},
    "attributes": {
        "platform": {
            "type": "enumeration",
            "required": true,
            "default": "Other",
            "enum": [
                "Facebook",
                "Twitter",
                "Instagram",
                "LinkedIn",
                "GitHub",
                "Other"
            ]
        },
        "url": {
            "type": "string",
            "required": true,
            "regex": "^(https?://)?(www\\.)?([a-zA-Z0-9-]+\\.)+[a-zA-Z]{2,}(/[a-zA-Z0-9._-]+)*/?$"
        }
    },
    "config": {}
}
